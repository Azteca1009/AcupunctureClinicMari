.mod-keyvisualHead {
  background-image: url("<%= asset_path('home/home_image_01.jpg') %>");
}
.none {
  background: #d9eef7;
}
/*INFOMATIONとSCHEDULE***************/
.home-infoClear-in {
  -webkit-transition: 0.6s;
  -o-transition: 0.6s;
  transition: 0.6s;
}
.home-info-h {
  font-family: 'Roboto', sans-serif;
  letter-spacing: 0.1em;
  color: #1ba1e6;
  font-weight: bold;
}
.home-info-posts-item {
  border-bottom: 1px solid #E1E5E7;
  font-weight: bold;
}
/*SYMPTOMS***************/
.home-field-sec-h {
  margin-bottom: 0.5em;
  font-weight: bold;
  text-align: center;
}
.home-field-sec-h small {
  font-family: 'Roboto', sans-serif;
  font-size: 0.85rem;
  letter-spacing: 0.1em;
  color: #1ba1e6;
  display: block;
  margin-bottom: 0.5em;
}
.home-field-sec-links {
  padding-top: 30px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-around;
}
.home-field-sec-links-item {
  width: 21%;
  margin-bottom: 3%;
}
.home-field-sec-links-item a {
  display: block;
  background: #fff;
  padding: 20px;
  text-align: center;
  height: 100%;
}
.home-field-sec-links-item i {
  width: 40%;
  display: inline-block;
}
/*DIFFERENCE***************/
.home-difference {
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
}
.home-difference-h {
  font-family: 'Roboto', sans-serif;
  letter-spacing: 0.1em;
  color: #1ba1e6;
  text-align: center;
  margin-bottom: 0.7em;
  font-weight: bold;
}
.home-difference-lead {
  margin-bottom: 0.5em;
  text-align: center;
  font-weight: bold;
}
.home-difference-sec-lead {
  margin-bottom: 1em;
  font-weight: 500;
}
.my-clinic{
  background: #d9eef7;
}
/*OTHER***************/
.home-other-h {
  font-family: 'Roboto', sans-serif;
  letter-spacing: 0.1em;
  color: #1ba1e6;
  text-align: center;
  padding: 40px 0 50px;
  font-weight: bold;
}
.home-other {
  background: #d9eef7;
}
