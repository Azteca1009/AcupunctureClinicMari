/**カレンダー**/
#calender-wrapper {
  background-color: #fff;
}
.calender {
  table-layout: fixed;
  width:100%;
  border-collapse: separate;
  border: 1px solid #dedede;
}
.calender th,
.calender td {
  font-size: 1.2rem;
  font-weight: 500;
  padding: 10px 0;
  border: 1px solid #dedede;
  text-align: center;
  vertical-align: middle;
}
.calender th {
  background-color: #E1E5E7;
}
.calender th:first-child {
  background-color: #e05557;
  color: #fff;
}
.calender td:first-child {
  color: #e05557;
}
.calender th:last-child {
  background-color: #207bcf;
  color: #fff;
}
.calender td:last-child {
  color: #207bcf;
}
.holiday {
  color: #e05557;
  background-color: #FFCCCC;
}
td.half_holiday {
  background-color: #d9eef7;
  padding:0;
}
td.half_holiday div {
  font-size: 0.8rem;
}
.calender caption {
  margin:10px;
  padding: 10px;
  border: 5px solid #dedede;
  border-radius: 30px;
  font-weight: bold;
}
.home-difference-other-selection {
  margin-bottom: 3em;
}
.home-other-itaihari {
  background: url("<%= asset_path('home/home_other_01.jpg') %>") center no-repeat;
  background-size: cover;
  position: relative;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  letter-spacing: 0.2em;
  height: 100%
}
.mod-noteArchive-posts-cat {
  position: absolute;
  background: #fff;
  font-weight: bold;
}
/* 画面サイズ変更対応 ****************************************/
/***********************************************************/
/* width1299px以上の画面                                    */
/***********************************************************/
@media screen and (min-width: 1299px) {
  /*INFOMATIONとSCHEDULE***************/
  .home-infoClear {
    padding: 50px 0;
  }
  .home-infoClear-in {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
  }
  .home-info {
    width: 48%;
  }
  .home-info-h {
    margin-bottom: 1.4em;
  }
  .home-info-posts-item {
    padding: 19px 0;
  }
  .home-schedule {
    width: 48%;
  }
  /*SYMPTOMS***************/
  .home-field {
    position: relative;
  }
  .home-field-bg01 {
    background: #d9eef7;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
  .home-field-sec {
    position: relative;
    width: 100%;
    padding: 60px 0 50px;
  }
  .home-field-sec-h {
    font-size: 1.8rem;
  }
  .home-field-sec-links-item i {
    width: 116px;
  }
  .home-field-sec-links-txt {
    font-weight: 500;
  }
  /*DIFFERENCE***************/
  .home-difference {
    padding: 40px 0;
  }
  .home-difference-lead {
    font-size: 1.8rem;
  }
  .home-difference-sec-lead {
    font-size: 1.1rem;
    text-align: center;
  }
  table.home-difference-other-table {
    table-layout: fixed;
    width: 100%;
    text-align: center;
    font-weight: 500;
    margin-bottom: 4rem;
  }
  .home-difference-other-table th, .home-difference-other-table td {
    border-right: 1px solid #E1E5E7;
    padding: 10px;
    font-weight: 500;
  }
  .home-difference-other-table h2 {
    font-size: 1.5em;
    font-weight: 500;
  }
  .circle_cross {
    font-size: 1.5rem;
  }
  /*OTHER***************/
  .home-other {
    position: relative;
  }
  .home-other-in {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
  }
  .home-other-sec {
    position: relative;
    width: 525px;
    height: 525px;
    margin-bottom: 50px;
  }
  .calender {
    height: 84%;
  }
  .mod-noteArchive-posts-cat {
    font-size: 1.5rem;
    top: 24px;
  }
}

